$(function(){$(".navigation_header_hamburger").click(function(){var t=$(".navigation_overlay_menu").css("display")==="none"?0:$(".navigation_overlay_menu").height(),n=$(".navigation_overlay_menu").css("display")==="none"?$(".navigation_overlay_menu").height():0;$(".navigation_overlay_menu").css({display:"block",height:t,overflow:"hidden"});$(".navigation_overlay_menu").animate({height:n},400,function(){$(this).css({height:"",overflow:""});n===0&&$(this).css({display:"none"})})})})